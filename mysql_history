grant all on sonar.* to 'sonar'@'%' identified by 'sonar';
grant all privileges on 'sonar'@'localhost';
grant all privileges on 'sonar'@'localhost' identified by 'sonar';
grant all privileges on 'sonar@localhost' identified by 'sonar';
grant ALL on sonar.* to 'sonar'@'localhost';
grant ALL on sonar.* to 'admin'@'localhost';
flush privileges;
grant ALL on sonar.* to 'admin'@'localhost';
quite
create user 'sonar' identified by 'sonar';
